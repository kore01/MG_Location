<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/drawer_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        tools:openDrawer="start" android:visibility="visible">


    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" android:visibility="visible">


            <android.support.v7.widget.Toolbar
                    android:id="@+id/toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="?attr/actionBarSize"
                    android:background="@color/colorPrimary"
                    android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
                    android:visibility="visible"/>
            <TextView
                    android:text="Работно време"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView14" android:gravity="center"
                    android:textAppearance="@style/title" android:layout_marginBottom="20dp"
                    android:layout_marginTop="10dp"/>
            <TextView
                    android:text="Дирекция"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView39"
                    android:textAppearance="@style/title_exp" android:layout_marginLeft="15dp"
                    android:layout_weight="1" android:gravity="bottom|left"/>
            <TextView
                    android:text="09-14"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView45" android:layout_marginLeft="15dp"
                    android:textAppearance="@style/biggrey" android:layout_weight="1"
                    android:gravity="left"/>
            <TextView
                    android:text="Помощник-дирекция"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView40"
                    android:textAppearance="@style/title_exp" android:layout_marginLeft="15dp"
                    android:layout_weight="1" android:gravity="bottom|left"/>
            <TextView
                    android:text="09-14"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView46" android:layout_marginLeft="15dp"
                    android:textAppearance="@style/biggrey" android:layout_weight="1"
                    android:gravity="left"/>
            <TextView
                    android:text="Офис"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView41"
                    android:textAppearance="@style/title_exp" android:layout_marginLeft="15dp"
                    android:layout_weight="1" android:gravity="bottom|left"/>
            <TextView
                    android:text="09-14"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView47" android:layout_marginLeft="15dp"
                    android:textAppearance="@style/biggrey" android:layout_weight="1"
                    android:gravity="left"/>
            <TextView
                    android:text="Библиотека"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView42"
                    android:textAppearance="@style/title_exp" android:layout_marginLeft="15dp"
                    android:layout_weight="1" android:gravity="bottom|left"/>
            <TextView
                    android:text="09-14"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView48" android:layout_marginLeft="15dp"
                    android:textAppearance="@style/biggrey" android:layout_weight="1"
                    android:gravity="left"/>
            <TextView
                    android:text="Домакин"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView43"
                    android:textAppearance="@style/title_exp" android:layout_marginLeft="15dp"
                    android:layout_weight="1" android:gravity="bottom|left"/>
            <TextView
                    android:text="09-14"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView49" android:layout_marginLeft="15dp"
                    android:textAppearance="@style/biggrey" android:layout_weight="1"
                    android:gravity="left"/>
            <TextView
                    android:text="Счетоводител"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView44"
                    android:textAppearance="@style/title_exp" android:layout_marginLeft="15dp"
                    android:layout_weight="1" android:gravity="bottom|left"/>
            <TextView
                    android:text="09-14"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/textView50" android:layout_marginLeft="15dp"
                    android:textAppearance="@style/biggrey" android:layout_weight="1"
                    android:gravity="left"/>


        </LinearLayout>
        <android.support.design.widget.CoordinatorLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/clayout"
                android:layout_gravity="bottom|right"
        >
        </android.support.design.widget.CoordinatorLayout>
    </RelativeLayout>

    <android.support.design.widget.NavigationView
            android:id="@+id/nav_view"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            app:headerLayout="@layout/nav_header"
            app:itemTextAppearance="@style/txt_expandable"
            app:menu="@menu/drawer_menu" android:visibility="visible">
    </android.support.design.widget.NavigationView>

</android.support.v4.widget.DrawerLayout>


